{
  "release-type": "rust",
  "packages": {
    "crates/transcriptio": {
      "component": "transcriptio"
    },
    "crates/transcriptio-cli": {
      "component": "transcriptio-cli"
    },
    ".": {
      "releaseType": "rust",
      "packageName": "transcriptio",
      "draft": false,
      "bumpMinorPreMajor": false,
      "bumpPatchForMinorPreMajor": false,
      "changelogPath": "CHANGELOG.md",
      "versioning": "default"
    }
  },
  "plugins": [
    {
      "type": "cargo-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "core",
      "components": [
        "transcriptio",
        "transcriptio-cli"
      ]
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
